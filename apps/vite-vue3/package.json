{
  "name": "vite-vue3-demo",
  "version": "2.12.0",
  "private": true,
  "scripts": {
    "setup-vue-demi": "pnpm exec vue-demi-switch 3",
    "dev": "pnpm run setup-vue-demi && vite",
    "build": "pnpm run setup-vue-demi && run-p type-check build-only",
    "build:ci": "pnpm run setup-vue-demi && run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "tsparticles": "^3.0.0-beta.1",
    "@tsparticles/engine": "^3.0.0-beta.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "@tsparticles/vue": "workspace:^"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^20.4.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "minimatch": "^9.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vue-tsc": "^1.8.8",
    "vue-demi": "^0.14.5"
  }
}
